<h1>All Users</h1>

<h4>Add Partners to Session:</h4>
   <%= form_tag(pick_users_path(@session), method: :get) do %>
      <%= text_field_tag(:search, params[:search])%>
      <%= submit_tag ("Search")%>
    <%end%>

<ul>
  <% @users.each do |user| %>
    <div><%= link_to user.name, user_path(user) %> <%= link_to "Add Runner to this Session", add_users_path(@session, user)%> </div>
  <% end %>
</ul>
